{
    "defaultSeverity": "warn",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "semicolon": false,
      "whitespace": false,
      "space-before-function-paren": true,
      "prefer-const": false,
      "no-console": false,
      "max-classes-per-file": false,
      "no-async-without-await": true
    },
    "rulesDirectory": []
}