{
  "name": "chat-overlay-server",
  "version": "0.1.0",
  "private": true,
  "main": "dist/app.js",
  "exports": "./dist/app.js",
  "dependencies": {
    "@types/node": "^16.11.7",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "immutable": "^4.0.0",
    "masterchat": "file:../masterchat",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.15",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "typescript-rest": "^1.6.2"
  },
  "scripts": {
    "build": "npm run build:dependencies && npm run build:project",
    "build:dependencies": "cd ../masterchat && npm i && npm run build",
    "build:project": "tsc",
    "watch": "tsc --watch",
    "start": "nodemon --watch dist --inspect dist/app.js",
    "auth": "cd ../masterchat/extra/credential-fetcher && npm i && npm run start"
  },
  "devDependencies": {
    "@types/express": "^4.17.13"
  },
  "_moduleAliases": {
    "@rebel": "dist"
  }
}
